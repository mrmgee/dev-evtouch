<?xml version="1.0"?>
<schema version="0.3">
	<table name="btLightboxedImage">
		<field name="bID" type="I">
			<key />
			<unsigned />
		</field>
		<field name="fID" type="I">
			<unsigned />
			<default value="0" />
		</field>
		<field name="maxWidth" type="I">
			<unsigned />
			<default value="0" />
		</field>
		<field name="maxHeight" type="I">
			<unsigned />
			<default value="0" />
		</field>
		
		<field name="maxWidthLarge" type="I">
			<unsigned />
			<default value="0" />
		</field>
		<field name="maxHeightLarge" type="I">
			<unsigned />
			<default value="0" />
		</field>
		
	 
		<field name="altText" type="C" size="255">
		</field>
		
		<field name="display_caption" type="C" size="10">
		</field>
		
		<field name="disableLightbox" type="I1">
			<unsigned />
			<notnull />
			<default value="1" />
		</field>
		
		
	</table>
</schema>