		<field name="frameborder" type="I"></field>
		<field name="marginwidth" type="C" size="10"></field>
		<field name="marginheight" type="C" size="10"></field>
		<field name="scrolling" type="C" size="10"></field>
		<field name="align" type="C" size="10"></field>
		<field name="marginheight" type="C" size="10"></field>
